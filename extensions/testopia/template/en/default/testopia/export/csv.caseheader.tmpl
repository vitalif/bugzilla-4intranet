[%# Testopia Test Case CSV column header export template #%]

[% colsepchar = user.settings.csv_colsepchar.value %]

[% FOREACH column = displaycolumns %]
    [% IF column == 'case_id' %]
        [% column FILTER csv %]
    [% ELSE %]
        [% column FILTER remove('_id') FILTER csv %]
    [% END %]
    [% IF column != displaycolumns.last %]
        [% colsepchar %]
    [% END %]
[% END %]